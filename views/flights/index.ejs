<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight No.</th>
      <th>Departure Times</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f, i) { %>
      <tr>
        <td><%- f.airline %></td>
        <td><%= f.airport %></td>
        <td><%= i + 10 %></td>
        <td><%= f.departs %></td>
        <td><a href="./flights/<%= f._id %>">Details</a></td>
      </tr>
    <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>